#search
	.receptions
		.reception-search
			background-color #fafafa
			float left
			height 7em
			width 100%
			border-bottom 1px solid #D0E9E9
			border-top 1px solid #D0E9E9
			padding 1em 2em
			display table
			
			div
				display table-cell
#search
	.receptions
		.results
			padding-top: 2em
			width 100%
			float left
			margin 0px
			.results-table
				width 100%
				border-collapse collapse
				
				.results-head
					tr
						th
							border-bottom 1.5px solid #D0E9E9
							text-align justify
							
				.results-body
					tr
						td
							padding 1em 0em
							border-bottom 1px dashed #D0E9E9